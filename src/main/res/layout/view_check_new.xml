<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/root"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center_vertical"
    android:minWidth="@dimen/_64sdp"
    android:minHeight="@dimen/_32sdp"
    android:orientation="horizontal"
    android:paddingStart="@dimen/_4sdp"
    android:paddingEnd="@dimen/_4sdp">


    <com.tripian.trpcore.util.widget.ImageView
        android:id="@+id/imCheck"
        android:layout_width="@dimen/_25sdp"
        android:layout_height="@dimen/_25sdp"
        app:srcCompat="@drawable/ic_check_empty_new" />

    <com.tripian.trpcore.util.widget.TextView
        android:id="@+id/tvText"
        style="@style/TextHeader2Regular"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_8sdp"
        android:layout_marginEnd="@dimen/_4sdp"
        android:textColor="@color/black"
        tools:text="deneme" />
</LinearLayout>